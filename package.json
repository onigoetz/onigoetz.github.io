{
  "dependencies": {
    "@svgr/webpack": "8.1.0",
    "contentful": "10.11.3",
    "dotenv": "16.4.5",
    "isomorphic-unfetch": "4.0.2",
    "marked": "12.0.2",
    "next": "14.2.3",
    "normalize.css": "8.0.1",
    "prettier": "3.2.5",
    "react": "18.3.1",
